{
	"manifest_version": 2,
	"name": "Discord Bot Client",
	"version": "1.0.0",
	"description": "This extension allows you to use https://botclient.tk",
	"author": "Flam3rboy",
	"background": {
		"scripts": ["script.js"]
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"https://botclient.tk/",
		"https://*.discord.com/",
		"wss://gateway.discord.gg/"
	],
	"icons": {
		"16": "images/16.png",
		"32": "images/32.png",
		"48": "images/48.png",
		"96": "images/96.png",
		"128": "images/128.png",
		"256": "images/256.png"
	}
}
